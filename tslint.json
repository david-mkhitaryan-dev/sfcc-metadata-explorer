{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "curly": true,
      "indent": [true, "spaces", 2],
      "no-console": false,
      "object-literal-key-quotes": false,
      "quotemark": [
        true,
        "single"
      ],
      "semicolon": true,
      "trailing-comma": false,
    },
    "rulesDirectory": []
}